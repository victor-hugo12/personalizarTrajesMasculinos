    "@types/react-test-renderer": "~18.0.7",
    "eslint": "~8.57.0",
    "eslint-config-expo": "~7.1.2",
    "eslint-config-prettier": "~9.1.0",
    "eslint-import-resolver-typescript": "~3.6.3",
    "eslint-plugin-prettier": "~5.2.1",
    "husky": "~9.1.6",
    "jest": "~29.4.0",
    "jest-expo": "~51.0.3",
    "prettier": "~3.3.3",
    "react-test-renderer": "~18.2.0",
