    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "formik": "~2.4.6",
    "i18n-js": "~4.5.0",
    "moment": "^2.30.1",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-loading-spinner-overlay": "~3.0.1",
    "react-native-paper": "~5.12.5",
